{
  "name": "trovu-web",
  "version": "1.0.0",
  "description": "Trovu web interface.",
  "main": "",
  "scripts": {
    "lint:yaml": "npx yaml-lint 'data/**/*.yml'",
    "lint:js": "eslint -c .eslintrc.js src/js",
    "lint": "npm run lint:yaml && npm run lint:js",
    "test:unit": "jest --verbose env helper src/js/ ",
    "test:calls": "jest --verbose calls",
    "test": "npm run lint:yaml && npm run test:unit && npm run test:calls",
    "build:jsdoc": "jsdoc --configure jsdoc.json",
    "build": "rollup --config --environment INCLUDE_DEPS,BUILD:production",
    "watch": "rollup --config --watch",
    "serve": "http-server ./dist/ --port 8081 --cors",
    "compile-data": "node -r esm src/js/cli.js compile-data --output ./dist/public/data.json",
    "normalize-data": "node -r esm src/js/cli.js normalize-data",
    "clean": "rm -r dist/",
    "sync": "./deploy.sh",
    "deploy": "npm run clean && npm run build && npm run sync"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trovu/trovu-web.git"
  },
  "keywords": [
    "javascript",
    "shortcuts",
    "social",
    "bootstrap"
  ],
  "author": "Georg Jaehnig",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/trovu/trovu-web/issues"
  },
  "homepage": "https://github.com/trovu/trovu-web#readme",
  "jest": {},
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@cospired/i18n-iso-languages": "github:georgjaehnig/i18n-iso-languages#new-languages",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-html": "^0.2.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "commander": "^11.1.0",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-n": "^16.5.0",
    "eslint-plugin-promise": "^6.1.1",
    "esm": "^3.2.25",
    "http-server": "^14.1.1",
    "is-valid-domain": "^0.1.6",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^27.3.1",
    "jsdoc": "^4.0.2",
    "languagelist": "github:georgjaehnig/language-list",
    "luxon": "^3.3.0",
    "markdown-it-anchor": "^8.6.7",
    "prettier": "^3.1.0",
    "rollup": "^2.38.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-execute": "^1.1.1",
    "rollup-plugin-git-info": "github:vapier/rollup-plugin-git-info",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-watch": "^1.0.2",
    "sass": "^1.39.0",
    "yaml-lint": "^1.7.0"
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "bootstrap.native": "^3.0.14",
    "countries-list": "^2.6.1",
    "dayjs": "^1.11.7",
    "detect-browser": "^5.2.0",
    "js-yaml": "^4.1.0"
  }
}
